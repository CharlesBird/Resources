<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <!-- Sequences for sale.order -->
        <record id="seq_linyan_contract_buy" model="ir.sequence">
            <field name="name">Linyan Contract Buy</field>
            <field name="code">linyan.contract.buy</field>
            <field name="prefix">LYP%(y)s%(month)s%(day)s</field>
            <field name="padding">2</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_linyan_contract" model="ir.sequence">
            <field name="name">Linyan Contract</field>
            <field name="code">linyan.contract</field>
            <field name="prefix">LYS%(y)s%(month)s%(day)s</field>
            <field name="padding">2</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_logistics_move" model="ir.sequence">
            <field name="name">Logistics Move</field>
            <field name="code">logistics.move</field>
            <field name="prefix">STK%(y)s%(month)s%(day)s</field>
            <field name="padding">2</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_account_invoice_in" model="ir.sequence">
            <field name="name">Account Invoice In</field>
            <field name="code">account.invoice.in</field>
            <field name="prefix">AP%(y)s%(month)s%(day)s</field>
            <field name="padding">2</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_account_invoice_out" model="ir.sequence">
            <field name="name">Account Invoice Out</field>
            <field name="code">account.invoice.out</field>
            <field name="prefix">AR%(y)s%(month)s%(day)s</field>
            <field name="padding">2</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_linyan_payment_customer_invoice" model="ir.sequence">
            <field name="name">Payment Customer Invoice</field>
            <field name="code">linyan.payment.customer.invoice</field>
            <field name="prefix">AP%(y)s%(month)s%(day)s</field>
            <field name="padding">2</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_linyan_payment_supplier_invoice" model="ir.sequence">
            <field name="name">Payment Supplier Invoice</field>
            <field name="code">linyan.payment.supplier.invoice</field>
            <field name="prefix">AR%(y)s%(month)s%(day)s</field>
            <field name="padding">2</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</openerp>
